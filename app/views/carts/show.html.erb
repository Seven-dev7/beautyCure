<p>Panier</p>
<div class="card-service-show">
    <% @cart_services.each do |cart_service| %><br>
        <div>
            <%= cart_service.service.title %>
            <%= cart_service.service.description %>
            <%= cart_service.service.price %>
            <%= cart_service.service.duration %>
            <%= link_to 'Supprimer', cart_delete_cart_path(cart_service.id), method: :delete, class: "btn btn-primary"%>
        </div>
    <%end%>
</div>